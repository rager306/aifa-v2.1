{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "app/**/page.tsx",
    "app/**/layout.tsx",
    "app/**/default.tsx",
    "app/**/loading.tsx",
    "app/**/error.tsx",
    "app/**/route.ts",
    "app/manifest.ts",
    "app/robots.ts",
    "app/sitemap.ts",
    "app/global-not-found.tsx"
  ],
  "project": [
    "**/*.{js,jsx,ts,tsx,mjs}",
    "!**/*.test.{js,jsx,ts,tsx}",
    "!**/*.spec.{js,jsx,ts,tsx}"
  ],
  "ignore": [
    "node_modules/**",
    ".next/**",
    "out/**",
    "build/**",
    "dist/**",
    "coverage/**",
    "*.config.{js,mjs,ts}",
    "next-env.d.ts",
    ".vercel/**",
    "public/**",
    "doc/**",
    ".serena/**"
  ],
  "ignoreDependencies": [
    "@eslint/eslintrc",
    "eslint-config-next",
    "tw-animate-css",
    "@radix-ui/react-accordion",
    "@radix-ui/react-progress",
    "@radix-ui/react-tooltip",
    "@xyflow/react",
    "embla-carousel-react",
    "jose",
    "next-pwa",
    "react-resizable-panels",
    "rehype-pretty-code",
    "shiki",
    "tokenlens"
  ],
  "next": {
    "entry": [
      "next.config.mjs",
      "app/layout.tsx",
      "app/@left/layout.tsx",
      "app/@rightStatic/layout.tsx",
      "app/@rightDynamic/layout.tsx"
    ]
  },
  "typescript": {
    "entry": ["tsconfig.json"]
  },
  "eslint": {
    "entry": ["eslint.config.mjs"]
  },
  "tailwind": {
    "entry": ["tailwind.config.ts"]
  },
  "postcss": {
    "entry": ["postcss.config.mjs"]
  }
}
