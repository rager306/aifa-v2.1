{
  "name": "aifa-v2.1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage && open coverage/index.html",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project=mobile-chrome",
    "test:e2e:no-js": "playwright test --project=no-js",
    "test:e2e:report": "playwright show-report",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "biome:ci": "biome ci .",
    "lint:all": "npm run lint && npm run biome:check",
    "knip": "knip",
    "knip:files": "knip --include files",
    "knip:exports": "knip --include exports",
    "knip:deps": "knip --include dependencies",
    "knip:report": "knip --reporter json > knip-report.json",
    "snyk:test": "snyk test --severity-threshold=medium",
    "snyk:monitor": "snyk monitor --severity-threshold=medium",
    "snyk:test:json": "snyk test --json > snyk-test-report.json",
    "snyk:fix": "snyk fix",
    "snyk:protect": "snyk protect",
    "semgrep": "uvx --from semgrep semgrep scan --config=.semgrep.yml --error",
    "semgrep:ci": "uvx --from semgrep semgrep scan --config=.semgrep.yml --error --json --output=semgrep-report.json",
    "semgrep:sarif": "uvx --from semgrep semgrep scan --config=.semgrep.yml --sarif --output=semgrep.sarif",
    "semgrep:auto": "uvx --from semgrep semgrep scan --config=auto",
    "semgrep:ai": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='components/ai-elements/**' --include='app/api/chat/**'",
    "semgrep:auth": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='app/@left/(_AUTH)/**'",
    "semgrep:api": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='app/api/**'",
    "prepare": "lefthook install",
    "lefthook:install": "lefthook install",
    "lefthook:uninstall": "lefthook uninstall",
    "lefthook:run:pre-commit": "lefthook run pre-commit",
    "lefthook:run:pre-push": "lefthook run pre-push",
    "git:safe:diff": "bash scripts/git-safe-diff.sh",
    "git:safe:ops": "bash scripts/git-safe-operations.sh",
    "git:escaping:demo": "bash scripts/git-escaping-demo.sh",
    "analyze": "ANALYZE=true next build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server next build",
    "analyze:both": "ANALYZE=true BUNDLE_ANALYZE=both next build",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --collect.settings.preset=desktop",
    "lighthouse:mobile": "lhci autorun --collect.settings.preset=mobile",
    "lighthouse:report": "lhci open"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc --noEmit --pretty false",
      "biome check --write --no-errors-on-unmatched",
      "eslint --fix --max-warnings=0"
    ],
    "*.{ts,tsx,js,jsx}": [
      "vitest related --run --passWithNoTests"
    ],
    "lib/**/*.ts": [
      "knip --include files"
    ],
    "components/**/*.{ts,tsx}": [
      "knip --include files"
    ],
    "app/**/*.{ts,tsx}": [
      "knip --include files"
    ]
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.77",
    "@ai-sdk/react": "^2.0.87",
    "@next/third-parties": "^16.0.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@types/js-cookie": "^3.0.6",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.7",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.3.1",
    "@xyflow/react": "^12.9.2",
    "ai": "^5.0.87",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "jose": "^6.1.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.553.0",
    "motion": "^12.23.24",
    "nanoid": "^5.1.6",
    "next": "^15.5.9",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-resizable-panels": "^3.0.6",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "2.15.4",
    "rehype-pretty-code": "^0.14.1",
    "shiki": "^3.14.0",
    "sonner": "^2.0.7",
    "streamdown": "^1.4.0",
    "tailwind-merge": "^3.3.1",
    "tokenlens": "^1.3.1",
    "use-stick-to-bottom": "^1.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^16.0.8",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9",
    "eslint-config-next": "15.5.7",
    "jsdom": "^27.3.0",
    "knip": "^5.71.0",
    "lefthook": "^2.0.8",
    "lint-staged": "^16.2.7",
    "snyk": "^1.1301.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^4.0.15"
  }
}
