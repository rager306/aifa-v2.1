{
  "name": "aifa-v2.1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "biome:check": "biome check .",
    "biome:fix": "biome check --write .",
    "biome:format": "biome format --write .",
    "biome:lint": "biome lint .",
    "biome:ci": "biome ci .",
    "lint:all": "npm run lint && npm run biome:check",
    "knip": "knip",
    "knip:files": "knip --include files",
    "knip:exports": "knip --include exports",
    "knip:deps": "knip --include dependencies",
    "knip:report": "knip --reporter json > knip-report.json",
    "snyk:test": "snyk test --severity-threshold=medium",
    "snyk:monitor": "snyk monitor --severity-threshold=medium",
    "snyk:test:json": "snyk test --json > snyk-test-report.json",
    "snyk:fix": "snyk fix",
    "snyk:protect": "snyk protect",
    "semgrep": "uvx --from semgrep semgrep scan --config=.semgrep.yml --error",
    "semgrep:ci": "uvx --from semgrep semgrep scan --config=.semgrep.yml --error --json --output=semgrep-report.json",
    "semgrep:sarif": "uvx --from semgrep semgrep scan --config=.semgrep.yml --sarif --output=semgrep.sarif",
    "semgrep:auto": "uvx --from semgrep semgrep scan --config=auto",
    "semgrep:ai": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='components/ai-elements/**' --include='app/api/chat/**'",
    "semgrep:auth": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='app/@left/(_AUTH)/**'",
    "semgrep:api": "uvx --from semgrep semgrep scan --config=.semgrep.yml --include='app/api/**'"
  },
  "dependencies": {
